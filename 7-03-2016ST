data (AirPassengers)

panam <- (AirPassengers)

class(panam)

start(panam); end(panam); frequency(panam)

panam

ts()### funcion para declarar una base de datos
##como serie de tiempo

layout(1:2)
plot(panam, main="Numero de Pasajeros aereos en Estados Unidos, 1949-1960", ylab="Pasajeros (miles)", xlab="AÃ±os")

plot (aggregate(panam)) ## sireve para obtenr la linea de tiempo

