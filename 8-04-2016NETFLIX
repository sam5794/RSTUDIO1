###NETFLIX

nfix <- read.csv("C://Users//SALA-C21//Documents//NETFLIX.csv", header = T)
nfixts <- ts(nfix [,5], start = 2015, frequency = 12)
plot.ts (nfixts)

Acf(nfixts)
 xx<- Acf(nfixts, main="Acf of quarterly beer production")
